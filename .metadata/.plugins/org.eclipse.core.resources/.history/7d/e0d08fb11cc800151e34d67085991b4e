class A08{}
class A081 extends A08{}
class A082 extends A081{}
public class Test08 {
	public static void method(Object obj){
		Class c=obj.getClass();
		while(!c.equals(Object.class)){
			System.out.println(obj.getClass());
			c=c.getSuperclass();
		}
	}
	public static void main(String args[]){
		A082 a=new A082();
		method(a);
	}
}
