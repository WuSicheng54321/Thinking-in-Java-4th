class Annoyance extends Exception{}
class Sneeze extends Annoyance{}

public class Human {
	public static void main(String args[]) throws Sneeze{
		try{
			throw new Sneeze();
		}catch (Annoyance a) {
				System.out.println("Caught Annoyance");
		}
		try{
			throw new Sneeze();
		}catch(Annoyance a){
			System.out.println("Caught Annoyance");
		}
	}
}
