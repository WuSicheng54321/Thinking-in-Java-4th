
public class GenericMethods {
	public <T,D,C> void f(T x,D d,C c){
		System.out.println(x.getClass().getSimpleName());
	}
	public static void main(String args[]){
		GenericMethods gm=new GenericMethods();
		gm.f(" "," "," ");
		gm.f(1,1,1);
		gm.f(1.0,1.0,1.0);
		gm.f(1.0F,1.0F,1.0F);
		gm.f('C','D','F');
		gm.f(gm,gm,gm);
	}
}
